fn assert(eq bool) = if !eq: @instr("process.exit(1)")

fn main =
    let a = 2
    let b = 4

    assert(a + b == 6)
    assert(a - b == -2)
    assert(a * b == 8)
    assert(b / 2 == 2)

    mut x = 23
    x += 1
    assert(x == 24)
    x -= 1
    assert(x == 23)
    x *= 2
    assert(x == 46)
    x /= 2
    assert(x == 23)
