:: x: ~Printable
fn print => @js"console.log(x)"

:: x: [int]
:: f: fn(int -> int)
fn map: [int] => @js"return x.map(f);"

:: arr: [~Printable]
:: n: int
fn print_n_elements =>
    mut i = 0

    for elem in arr =>
        if i >= n => break
        i += 1
        print(elem)

:: x: int
fn add1 => return x + 1

:: x: int
fn square => return x * x

let arr: [int] = [
    1, 
    2, 
    square(3), 
    4, 5,
].map(add1)

let _ = arr
    .map(square)
    .print_n_elements(3)