:: x: string
fn print => 
    if x == 1 => 
        @js"console.log(x)"
    else 
        @js"console.log('siema ' + x)"

:: x: [int]
:: f: (int -> int)
fn map: [int] => @js"return x.map(f);"

:: arr: [any]
:: n: int
fn print_n_elements =>
    mut i = 0
    for elem in arr =>
        if i >= n => break
        i = i + 1
        print(elem)

:: x: int
fn square => return x * x

let arr: [int] = [1, 2, 3, 4, 5]

arr.map(square).print_n_elements(3)
