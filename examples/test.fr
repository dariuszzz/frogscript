:: str: string
:: n: int
fn repeat => @js"return str.repeat(n);"

:: x: string
fn print => @js"console.log(x)"

fn mozambik => return "mozambik"

:: x: int
fn afryka => 
    if x > 5 =>
        return "za duzo mozambikuw"
    else 
        let mozambik2 = mozambik().repeat(x)
        return "siemaelo320 -- " + mozambik2

afryka(5).print()
afryka(10).print()

